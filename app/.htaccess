Options +FollowSymLinks -MultiViews
# Turn mod_rewrite on
RewriteEngine On
RewriteBase /


## EXPIRES CACHING ##
<IfModule mod_expires.c>
ExpiresActive On
ExpiresByType index.html "access 1 month"

</IfModule>
## EXPIRES CACHING ##

<IfModule mod_deflate.c>
    <filesMatch "\.(js|css|html)$">
        SetOutputFilter DEFLATE
    </filesMatch>
</IfModule>



